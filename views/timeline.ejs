<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Internet History Timeline</title>
    <link rel="stylesheet" href="/style.css" />
    <script src="/script.js" defer></script>
  </head>
  <body>
    <header>
      <nav aria-label="Primary navigation">
        <ul>
          <li><a href="/timeline">Timeline</a></li>
          <li><a href="/early-isps">Early ISPs</a></li>
          <li><a href="/commercial-provision">Commercial Provision</a></li>
        </ul>
      </nav>
      <h1>Timeline of the Internet in the UK</h1>
    </header>

    <main>
      <section class="timeline">
        <ul>
          <% timelineData.forEach(item => { %>
            <li>
              <div>
                <h2><%= item.title %></h2>
                <h3><%= item.year %></h3>
                <p><%= item.content %></p>
                <p><strong>Source:</strong> 
                  <a href="<%= item.source %>" target="_blank">
                    <%= item.source %>
                  </a>
                </p>
              </div>
            </li>
          <% }); %>
        </ul>
      </section>
    </main>
  </body>
</html>
